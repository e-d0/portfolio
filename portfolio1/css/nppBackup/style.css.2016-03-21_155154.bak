/* animation for header_scroll */
@keyframes icon_anim {
0% {margin-top: 240px;}
100% {margin-top: 260px;}
}
@-moz-keyframes icon_anim {
0% {margin-top: 240px;}
100% {margin-top: 260px;}
}
@-webkit-keyframes icon_anim{
0% {margin-top: 240px;}
100% {margin-top: 260px;}
}

/*-----all documenr properties------*/
