#header_scroll{
margin-top: 250px;
-webkit-animation: icon_anim 1s infinite;
-o-animation: icon_anim 1s infinite;
animation: icon_anim 1s infinite;
-webkit-animation-direction: alternate;
-moz-animation-direction: alternate;
-o-animation-direction: alternate;
animation-direction: alternate;
}
